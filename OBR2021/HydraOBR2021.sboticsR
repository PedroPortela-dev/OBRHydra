tarefa obstaculo{

	se(ultra(1)<=13)entao{
		escrever(1,"Obstaculo")
		alinhamento()
		se(ultra(2)<=40)entao{
			rotacionar(500,negativo(90))
			obstaculo2()
			rotacionar(500,90)
			obstaculo2()
			se(frontal >sensibilidade)entao{
				rotacionar(500,90)
				obstaculo2()
				se(frontal >sensibilidade)entao{
				rotacionar(500,90)
				obstaculo2()
				}
			}
			esquerda(1000)
		}
		senao{

			rotacionar(500,90)
			obstaculo1()
			rotacionar(500,negativo(90))
			obstaculo1()
			se(frontal >sensibilidade)entao{
				rotacionar(500,negativo(90))
				obstaculo1()
				se(frontal >sensibilidade)entao{
					rotacionar(500,negativo(90))
					obstaculo1()
				}
			}
			direita(1000)
		}
		enquanto(luz(2)>=sensibilidade)farei{}
		frento=verdadeiro
		timer = tempo()
	}
}
